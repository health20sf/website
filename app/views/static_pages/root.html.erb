<div class="section section-alt" id="features">
  <div class="container">
    <div class="content">
      <div class="row">
        <div class="span12">
          <div class="title">
            <h2>Events</h2>
            <div class="hr hr-small hr-center"><span class="hr-inner"></span></div>
            <p></p>
          </div>
        </div>
      </div>
      <div class="row">
          <table class="table table-bordered table-striped">
            <thead>
              <tr>
                <th>Month</th>
                <th>Title</th>
                <th>Description</th>
                <th>Registration</th>
              </tr>
            </thead>
            <tbody>
              <% @events.each do |event| %>
              <tr>
                <td><%= event.title %></td>
                <td><%= event.location %></td>
                <td><%= event.url %></td>
                <td><a href="https://www.eventbrite.com/e/april-chapter-meeting-health-20-san-francisco-media-panel-tickets-22746938706?ref=estw">Eventbrite</a></td>
              </tr>
              <% end %>
              <tr>
                <td>June</td>
                <td>Holly Potter</td>
                <td>Learn how to raise funds for your health IT company</td>
                <td><a href="https://www.eventbrite.com/e/june-chapter-meeting-health-20-san-francisco-tickets-18947252743">Eventbrite</a></td>
              </tr>
              <tr>
                <td>July</td>
                <td>Parag Vaish</td>
                <td>Learn how to innovate your health IT company</td>
                <td><a href="https://www.eventbrite.com/e/july-chapter-meeting-health-20-san-francisco-tickets-21995579370">Eventbrite</a></td>
              </tr>

            </tbody>
          </table>
      </div>
    </div>
  </div>
</div>


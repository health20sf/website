$(document).ready(function() {
  $('.modal-launch').click(function() {
    $('.modal').show();
    $('.modal-mask').show();
  });

  $(document).ready(function () {
    setTimeout(function(){$(".modal, .modal-mask").fadeIn(); }, 50);
  });

  $( ".modal" ).append( $( ".hbspt-form" ) );
   $('.modal-close').click(function(){
      $('.modal').hide();
      $('.modal-mask').hide();
  });
});
</script>


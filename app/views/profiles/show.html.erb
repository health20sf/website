      <% provide(:title, 'User Profile') %>
      <% provide(:button_text, 'User Profile') %>
      <h1>User Profile</h1>
      <br />
      <p>
        First name: <%=@profile.first_name %>
      </p>
      <p>
        Last name: <%=@profile.last_name %>
      </p>
